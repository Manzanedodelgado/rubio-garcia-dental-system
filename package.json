{
  "name": "rubio-garcia-dental",
  "version": "2.1.0",
  "private": true,
  "description": "Sistema de Gestión Dental - Rubio García Dental con GESDEN ↔ Supabase Integration",
  "engines": {
    "node": ">=18.19.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose --coverage",
    "test:integration": "vitest run test/integration --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:security": "vitest run test/security --reporter=verbose",
    "test:performance": "vitest run test/performance --reporter=verbose --testTimeout=60000",
    "test:a11y": "playwright test test/accessibility",
    "test:mobile": "playwright test test/mobile",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:critical": "vitest run test/critical-system-fixed.test.ts --reporter=verbose",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:security",
    "install:clean": "rm -rf node_modules package-lock.json && npm install",
    "install:prod": "npm ci --only=production",
    "clean": "rm -rf .next dist coverage node_modules",
    "clean:cache": "rm -rf .next .cache",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "health-check": "node scripts/health-check.js",
    "setup": "./install-gesden.sh",
    "verify": "./verify-gesden.sh"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.17.0",
    "@types/node": "^18.19.0",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@whiskeysockets/baileys": "^6.6.0",
    "axios": "^1.6.5",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "libphonenumber-js": "^1.10.53",
    "lucide-react": "^0.309.0",
    "moment": "^2.29.4",
    "mssql": "^10.0.2",
    "next": "14.0.4",
    "pino": "^8.17.2",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-calendar": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.10.3",
    "socket.io-client": "^4.5.4",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react-big-calendar": "^1.8.9",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "jsdom": "^23.0.1",
    "lighthouse": "^11.4.0",
    "msw": "^2.0.11",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.8",
    "vitest": "^1.0.4",
    "@vitest/coverage-v8": "^1.0.4"
  },
  "keywords": [
    "dental",
    "gestion",
    "gesden",
    "supabase",
    "whatsapp",
    "ai",
    "nextjs",
    "typescript",
    "dental-management",
    "clinic-management"
  ],
  "author": {
    "name": "MiniMax Agent",
    "email": "info@rubiogarciadental.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Manzanedodelgado/rubio-garcia-dental-system"
